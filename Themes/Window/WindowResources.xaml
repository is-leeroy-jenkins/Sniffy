<ResourceDictionary
    x:Class="Badger.WindowResources"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Badger">
    <SolidColorBrush
        x:Key="BackColor"
        Color="#FF141414" />
    <SolidColorBrush
        x:Key="ForeColor"
        Color="#FFDEDEDE" />
    <SolidColorBrush
        x:Key="BorderColor"
        Color="#FF0078D4" />
    <SolidColorBrush
        x:Key="Transparent"
        Color="#00000000" />
    <SolidColorBrush
        x:Key="ControlBackColor"
        Color="#FF2D2D2D" />
    <SolidColorBrush
        x:Key="LightBlueColor"
        Color="#FF6ABDFC" />
    <SolidColorBrush
        x:Key="WhiteColor"
        Color="#FFFFFFFF" />
    <SolidColorBrush
        x:Key="DarkBlueColor"
        Color="#FF325D81" />
    <SolidColorBrush
        x:Key="SteelBlueColor"
        Color="SteelBlue" />
    <SolidColorBrush
        x:Key="RedColor"
        Color="Maroon" />

</ResourceDictionary>
